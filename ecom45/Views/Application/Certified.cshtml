                    <label>
                        @Html.RadioButtonFor(m => m.YN_RefService, true) Yes
                    </label>
                </div>
                <div class="radio">
                    <label>
                        @Html.RadioButtonFor(m => m.YN_RefService, false) No
                    </label>
                </div>
                @Html.TextAreaFor(m => m.YN_RefService_Remark, new { @class = "form-control", placeholder = "Reference" })
            </div>
        </div>

        <div class="form-group">
            <label for="" class="col-sm-3 control-label">
                27. Does your company currently carry FSL Insurance (Errors and Omissions and Legal Liability Coverage)?
            </label>
            <div class="col-sm-9">
                <div class="radio">
                    <label>
                        @Html.RadioButtonFor(m => m.EandO, true) Yes
                    </label>
                </div>
                <div class="radio">
                    <label>
                        @Html.RadioButtonFor(m => m.EandO, false) No
                    </label>
                </div>
                @Html.TextBoxFor(m => m.EandO_Remark, new { @class = "form-control", placeholder = "Remark" })
            </div>
        </div>

        <div class="form-group">
            <label for="" class="col-sm-3 control-label">
