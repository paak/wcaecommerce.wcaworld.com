        <div class="form-group">
            <label for="Cid" class="col-sm-3 control-label">
                1. WCA/GAA/Lognet Member ID Number:
            </label>
            @*@Html.LabelFor(m => m.Cid, new { @class = "col-sm-3 control-label" })*@
            <div class="col-sm-2">
                @Html.TextBoxFor(m => m.Cid, new { @class = "form-control col-sm-1", @readonly = "true" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">
                2. List the networks you are a part of and the date your membership(s) started:
            </label>
            <div class="col-sm-9">
                <ul class="list-unstyled">
                    <li>WCA (10/3/2009)</li>
                    <li>APLN (11/6/2009)</li>
                    <li>CGLN (2/18/2009)</li>
                </ul>
            </div>
        </div>

        <div class="form-group">
            <label for="Company" class="col-sm-3 control-label">
                3. Company Name:
            </label>
            <div class="col-sm-9">
                @Html.TextBoxFor(m => m.Company, new { @class = "form-control" })
            </div>
        </div>

        <div class="form-group">
            <label for="AddressHQ" class="col-sm-3 control-label">
                4. Company Head office Address:
            </label>
