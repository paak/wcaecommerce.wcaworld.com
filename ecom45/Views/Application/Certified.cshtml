            <div class="col-sm-9">
                <div class="radio">
                    <label>
                        @Html.RadioButtonFor(m => m.YN_CyberInsur, "true") Yes
                    </label>
                </div>
                <div class="radio">
                    <label>
                        @Html.RadioButtonFor(m => m.YN_CyberInsur, "false") No
                    </label>
                </div>
                @Html.TextBoxFor(m => m.YN_CyberInsur_Remark, new { @class = "form-control", placeholder = "Remark" })
            </div>
