                29. Do you currently offer your customers cargo insurance?
            </label>
            <div class="col-sm-9">
                <div class="radio">
                    <label>
                        @Html.RadioButtonFor(m => m.YN_CargoInsur, "true") Yes
                    </label>
                </div>
                <div class="radio">
                    <label>
                        @Html.RadioButtonFor(m => m.YN_CargoInsur, "false") No
                    </label>
                </div>
                @Html.TextBoxFor(m => m.YN_CargoInsur_Remark, new { @class = "form-control", placeholder = "Remark" })
            </div>
        </div>
